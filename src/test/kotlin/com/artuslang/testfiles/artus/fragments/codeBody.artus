/*
    this is the base fragment where all functionality meant to generate code goes, adding matchers is the way to go.
    any context added to this can add to or define "artus.code.builder" in properties, it is an array list of <function(Data){}> Data as declared by lang:data()
*/
    var root = "../../";
    lang:import(root + "utils/contextUtil.artus");
    lang:import(root + "utils/typesUtil.artus");
    lang:import("ignore.artus");

    var ignore = contexts:get("artus.ignore");
    var codeBodyCtx = lang:contextType("artus.code.body", lang:arrayListOf(), lang:arrayListOf(ignore));

    contexts:put("artus.code.body", codeBodyCtx);

    context.type.addParent(codeBodyCtx);
